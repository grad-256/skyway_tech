{
  "name": "skyway_tech",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc",
    "generate": "tsc --noEmit",
    "format": "prettier --write ./src",
    "terraform:init": "cd terraform-aws-project && terraform init",
    "terraform:apply": "cd terraform-aws-project && terraform apply",
    "terraform:destroy": "cd terraform-aws-project && terraform destroy",
    "terraform:plan": "cd terraform-aws-project && terraform plan"
  },
  "devDependencies": {
    "@vitest/ui": "^0.32.4",
    "typescript": "^5.7.2",
    "vite": "^4.5.5",
    "vitest": "^0.32.4"
  },
  "dependencies": {
    "@skyway-sdk/core": "^1.10.0",
    "@skyway-sdk/room": "^1.10.0",
    "@skyway-sdk/sfu-bot": "^1.10.0",
    "@skyway-sdk/token": "^1.6.3",
    "@types/node": "^22.10.1",
    "cros": "^1.0.1",
    "crypto": "^1.0.1",
    "express": "^4.21.2",
    "prettier": "^3.4.2"
  }
}
